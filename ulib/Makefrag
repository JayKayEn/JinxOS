LIBS += $(OBJDIR)/ulib/syscall.o
LIBS += $(OBJDIR)/ulib/print.o
LIBS += $(OBJDIR)/ulib/string.o
LIBS += $(OBJDIR)/ulib/usermain.o

$(OBJDIR)/ulib/%.o: ulib/%.c
	@$(CC) $(UFLAGS) -O3 -c -o $@ $< > /dev/null
	@echo " <+> $<"

$(OBJDIR)/ulib/libu.a: $(LIBS)
	@$(AR) r $@ $(LIBS) 2> /dev/null
	@echo " <+> libu.a"
